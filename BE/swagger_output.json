                        "properties": {
                          "displayName": {
                            "type": "string",
                            "example": "English"
                          },
                          "code": {
                            "type": "string",
                            "example": "eng"
                          }
                        }
                      }
                    },
                    "Currency": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "displayName": {
                            "type": "string",
                            "example": "Euro"
                          },
                          "code": {
                            "type": "string",
                            "example": "EUR"
                          }
                        }
                      }
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
