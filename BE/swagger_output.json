    "/api/v1/user/": {
      "get": {
        "tags": [
          "User"
        ],
        "description": "Using MSAL token we can get info of authorized user",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "FirstName": {
                      "type": "string",
                      "example": "Karuppaiya"
                    },
                    "MiddleName": {},
                    "LastName": {
                      "type": "string",
                      "example": "P"
                    },
                    "EmailID": {
                      "type": "string",
                      "example": "karuppaiya.p@unilever.com"
                    },
                    "UserPersona": {
                      "type": "string",
                      "example": "SalesManager"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": false
                },
                "message": {
                  "type": "string",
                  "example": "Token Expired"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
